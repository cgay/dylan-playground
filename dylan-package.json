{
    "name": "dylan-playground",
    "description": "Web-based playground in which to build and run Dylan code",
    "version": "0.1",
    "dependencies": [
        # Note that these deps need to include all the libraries used by
        # the generated code as well as by the playground code itself.
        # See play.dylan
        "hash-algorithms@1.0",
        "http@1.1.1",
        "json@1.0",
        "logging@2.2",
        "regular-expressions@0.2",
        "strings@1.2",
        "uuid@0.1",
        "xml-parser@0.1"
    ],
    "dev-dependencies": [
        "testworks"
    ],
    "url": "https://github.com/dylan-lang/dylan-playground"
}
